    "@mui/styled-engine": "^5.0.1",
    "@mui/styles": "^5.0.1",
    "@reduxjs/toolkit": "^1.6.2",
    "chart.js": "^3.5.1",
    "i18next": "^21.2.6",
    "i18next-browser-languagedetector": "^6.1.2",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.5",
    "react-dom": "^17.0.2",
