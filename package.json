  "lint-staged": {
    "src/**/*.ts": [
      "eslint",
      "git add"
    ]
  },
  "alias": {
    "components": "./src/common/components",
    "features": "./src/features",
    "store": "./src/app/store",
    "api": "./src/api/api",
    "styles": "./src/styles",
