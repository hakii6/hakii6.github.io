{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver", {
        "extensions": [
          ".ts",
          ".tsx",
          ".jsx",
          ".js",
          ".json"
        ],
        "alias": {
          "features/*": ["./src/features/*"],
          "store": ["./src/app/store"],
          "api": ["./src/api/api"],
          "styles": ["./src/styles"],
          "utils/*": ["./src/common/utils/*"],
          "constants/*": ["./src/common/constants/*"]
        }
      }
    ],
  ]
}
